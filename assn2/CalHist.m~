function [ s, varargout ] = CalHist(inputImage)
%CALHIST Summary of this function goes here
%   Detailed explanation goes here
    A = inputImage;
    dict = containers.Map
    
    [rows,cols] = size(A);
    %disp("There are " + rows + " rows and " + cols + "cols");
    countA = 1;
    for col = 1:cols
        for row = 1:rows
            if ~isKey(dict, A(row,col)) %if does not exist
                dict{A(row,col)} = 1;
                %disp(A(row,col));
            else
                dict{A(row,col)} = dict{A(row,col)} + 1; %increment 1 occurrence
                
            end
        end
    end 



    nout = max(nargout,1) - 1;
    s = size(inputImage);

    for k = 1:nout
        varargout{k} = s(k);
    end
end

